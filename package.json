{
  "name": "aqua-wise-shopify-function",
  "version": "0.1.0",
  "description": "Google Cloud Function to sync Aqua Wise catalog items to Shopify.",
  "main": "index.js",
  "scripts": {
    "start": "functions-framework --target=shopifyProductSync",
    "start:bundle": "functions-framework --target=createBundleProducts --source=bundles/AQUA-WISE/index.js"
  },
  "dependencies": {
    "@google-cloud/functions-framework": "^3.4.0",
    "dotenv": "^16.4.5"
  },
  "engines": {
    "node": ">=18"
  }
}
